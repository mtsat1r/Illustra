{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Illustra",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/src/bin/Debug/net9.0-windows/Illustra.dll",
      "args": [],
      "cwd": "${workspaceFolder}/src/",
      "console": "internalConsole",
      "stopAtEntry": false
    },
    {
      "name": "Attach to Process",
      "type": "coreclr",
      "request": "attach"
    }
  ]
}